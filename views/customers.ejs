<%- include layouts/header.ejs %>

<div class="page-data">
    <div class="data-btn">
        <button onClick="addUser();">+ Add Show</button>
    </div>
    <div class="data-table">
        <table border="1" cellpadding="7" cellspacing="7">
            <tr>
                <th width="50px">No</th>
                <th>Name</th>
                <th>Date Sessions</th>
                <th>Ranking</th>
                <th>Value</th>
                <th width="120px">Info</th>
            </tr>
            <% if(data.length){                                 
                 for(var i = 0;i < data.length;i++) { %> <tr>
                <td><%=(i+1)%></td>
                <td><%=data[i].name%></td>
                <td><%=data[i].date%></td>
                <td><%=data[i].ranking%></td>
                <td><%=data[i].value%></td>
                <td><%=data[i].info%></td>
                <td>
                    <a class="a-inside edit" href="../customers/edit/<%=data[i].id%>">Edit</a>
                    <a class="a-inside delete" href="../customers/delete/<%=data[i].id%>">Delete</a>
                </td>
            </tr>
            <% }
             }else{ %>
            <tr>
                <td colspan="6">No show</td>
            </tr>
            <% } %>
        </table>
    </div>
</div>

<%- include layouts/footer.ejs %>