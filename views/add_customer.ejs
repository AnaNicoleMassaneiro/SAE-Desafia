<%- include layouts/header.ejs %>

        <div class="page-data">
            
            <form method="post" action="/customers/add">
                <table cellpadding="11">
                <tr>
                    <td class="label">Name</td><td>: <input type="text" name="name" required></td>
                 </tr>
                 <tr>
                    <td class="label">Date Sessions</td><td>: <input type="text" name="date" required></td>
                 </tr>
                 <tr>
                    <td class="label">Ranking</td><td>: <input type="text" name="ranking" required></td>
                 </tr>        
                <tr>
                    <td class="label">Value</td><td>: <input type="text" name="value" required></td>
                 </tr>
                 <tr>
                    <td class="label" valign="top">Info</td><td valign="top">: <textarea name="info" required cols="30" rows="5"></textarea></td>
                </tr>
                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>
            
        </div>
        
<%- include layouts/footer.ejs %>
